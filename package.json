{
  "name": "proxystaff",
  "version": "1.0.0",
  "description": "ProxyStaff - AI-powered virtual assistant platform for realtors using WhatsApp",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "build": "prisma generate && tsc",
    "start": "prisma migrate deploy && node dist/index.js",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"scripts/**/*.ts\"",
    "mcp:tools": "tsx src/mcp/toolManifestServer.ts",
    "onboard": "tsx scripts/onboard.ts"
  },
  "keywords": [
    "proxystaff",
    "whatsapp",
    "ai-assistant",
    "realtor"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.1.0",
    "@types/pg": "^8.16.0",
    "bullmq": "^5.66.4",
    "chrono-node": "^2.9.0",
    "cron-parser": "^5.4.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.8.2",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "playwright": "^1.57.0",
    "prisma": "^7.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.22.0",
    "globals": "^15.15.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
